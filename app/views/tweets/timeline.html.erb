      <div id="container">
      <% @listing.each do |listing| %>
        <% if session[:user_id] %>
          <div class="element">
            <img src="<%= listing.user.profile_image_url =%>" />
            <h2><%= listing.user.name =%> (<%= "@"+listing.user.screen_name =%>)</h2>
            <%= linkify(listing.text).html_safe %> 
          </div>        
          <% else %>
          <p> Sign in first </p>
          <% end %>
      <% end %>
      </ul>
    </div>
